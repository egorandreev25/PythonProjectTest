   "source": [
    "# %load ODE_mini_project.py\n",
    "from matplotlib import pyplot as plt\n",
    "\n",
    "import sympy as sp\n",
    "import numpy as np\n",
    "\n",
    "y = sp.Function('y')\n",
    "t = sp.Symbol('t')\n",
    "\n",
    "dgl = sp.Eq(3 * y(t).diff(t) + 1, y(t))\n",
    "\n",
    "display(dgl)\n"
   ]
